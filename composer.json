{
    "name": "whitedigital-eu/audit-service",
    "description": "Audit Service",
    "type": "symfony-bundle",
    "keywords": [
        "api-platform",
        "audit",
        "php",
        "symfony",
        "symfony-bundle"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "ext-mbstring": "*",
        "api-platform/core": "^3.2",
        "doctrine/collections": "*",
        "doctrine/doctrine-bundle": "*",
        "doctrine/orm": "*",
        "doctrine/persistence": "*",
        "symfony/config": "^7.0",
        "symfony/console": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/event-dispatcher": "^7.0",
        "symfony/http-foundation": "^7.0",
        "symfony/http-kernel": "^7.0",
        "symfony/maker-bundle": "*",
        "symfony/security-bundle": "^7.0",
        "symfony/serializer": "^7.0",
        "symfony/translation": "^7.0",
        "symfony/validator": "^7.0",
        "whitedigital-eu/entity-resource-mapper-bundle": "^0.25"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "*",
        "roave/security-advisories": "dev-latest",
        "whitedigital-eu/config-pack": "^2.1"
    },
    "conflict": {
        "whitedigital-eu/entity-resource-mapper-bundle": "<0.25"
    },
    "autoload": {
        "psr-4": {
            "WhiteDigital\\Audit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WhiteDigital\\Audit\\Tests\\Source\\": "tests/src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.x-dev"
        }
    }
}
